{
  "phases": {
    "install": {
      "cmds": [
        "pip install -r requirements.txt"
      ]
    },
    "build": {
      "cmds": [
        "python manage.py collectstatic --noinput"
      ]
    },
    "start": {
      "cmds": [
        "python manage.py migrate",
        "gunicorn ecommerce.wsgi:application --bind 0.0.0.0:$PORT"
      ]
    }
  }
}
